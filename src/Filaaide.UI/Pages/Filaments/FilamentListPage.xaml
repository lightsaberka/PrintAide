	IconImageSource="filaments">

	<ContentPage.Content>
        <StackLayout>
            <Label Text="Filament list"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
        </StackLayout>
    </ContentPage.Content>

